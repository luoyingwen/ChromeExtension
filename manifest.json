{
   //Required
   "name": "devloper proxy",
   "version": "0.5",
   "manifest_version": 2,
   
   // Recommended
   "description": "developer online proxy",
   "icons": {
      "12": "icon.png",
      "128": "icon.png",
      "48": "icon.png"
   },

   // Pick one (or none)
   "page_action": {
      "default_icon": "icon.png",
      "default_title": "developer proxy"
   },
   
   // Add any of these that you need
   "background": 
   {
      "scripts": ["webproxymenu.js"]
   },
   "content_scripts": 
   [
	   {
	      "matches": [
		  "https://hellocockroach.appspot.com/*"
		  ],
		  
	      "js": ["webproxysitescript.js"],
	      "run_at":"document_start"
	   },
	   {
	      "matches": [
		  "http://www.google.com/*", 
		  "https://encrypted.google.com/*", 
		  "http://www.google.com.hk/*", 
		  "https://www.google.co.uk/*", 
		  "https://www.google.fr/*",
		  "https://www.google.de/*"
		  ],
		  
	      "js": ["disablegoogleredirect.js"],
	      "run_at":"document_end"
	   }	   
   ],
   "permissions": [ "tabs", "contextMenus", "cookies", "https://hellocockroach.appspot.com/*"]
}
